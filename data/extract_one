#!/usr/bin/env bash

START_ROUND_6=10245999
END_ROUND_6=10309068
#END_ROUND_6=latest

#export NO_COLOR=true
export API_MODE=true

chifra list $2 --start $START_ROUND_6  --end $END_ROUND_6 | grep -v "\"address\"" | jq '.' >appearances/$1/$2.json

chifra export $2 --articulate --start $START_ROUND_6 --end $END_ROUND_6 | jq '.' >transactions/$1/$2.json

chifra export --logs $2 --articulate --start $START_ROUND_6 --end $END_ROUND_6 | jq '.' >logs/$1/$2.json

sleep .2
